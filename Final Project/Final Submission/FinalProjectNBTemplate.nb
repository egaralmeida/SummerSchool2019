Cell[TextData[{
 StyleBox["void",
  FontColor->RGBColor[
   0.00784313725490196, 0.6862745098039216, 0.7254901960784313]],
 " ",
 StyleBox["loop",
  FontColor->RGBColor[
   0.3764705882352941, 0.6078431372549019, 0.39215686274509803`]],
 "() {\n\t",
 StyleBox["digitalWrite",
  FontColor->RGBColor[1., 0.5019607843137255, 0.]],
 "(LED_PIN, ",
 StyleBox["HIGH",
  FontColor->RGBColor[
   0.00784313725490196, 0.6862745098039216, 0.7254901960784313]],
 ");\t\t// Turn LED on\n\t",
 StyleBox["delay",
  FontColor->RGBColor[1., 0.5019607843137255, 0.]],
 "(500);\t\t\t\t\t// Wait for 500 mS (half a second)\n\t",
 StyleBox["digitalWrite",
  FontColor->RGBColor[1., 0.5019607843137255, 0.]],
 "(LED_PIN, ",
 StyleBox["LOW",
  FontColor->RGBColor[
   0.00784313725490196, 0.6862745098039216, 0.7254901960784313]],
 ");\t\t// Turn LED off\n\t",
 StyleBox["delay",
  FontColor->RGBColor[1., 0.5019607843137255, 0.]],
 "(500);\t\t\t\t\t// Wait for 500 mS again\n}"
}], "CodeText",
